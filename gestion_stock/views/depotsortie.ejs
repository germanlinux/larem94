<% include partial_hdp %>

<div id="page-wrapper">
                      <!-- /.col-lg-12 -->
        <div class="row">
            <div class="col-lg-12">
                    <h1 class="page-header">Approvisionner un depot secondaire </h1>
           </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
             <div class="panel panel-green">
                <div class="panel-heading">
                             Depuis le depot département
                </div>
                <form role="form" METHOD="POST">
             </div>
          </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="form-group">
                      <label>Produit</label>
                      <select name="produit" class="form-control">
                       <% for (var i=0; i < produits.length ; i++) {%>
                           <option value="<%= produits[i].id_produit%>"><%= produits[i].nom %> (<%= produits[i].stock %>) </option>
                       <% } %>
                      </select>
                </div>
            </div>
            <div class="col-lg-2">
                    <div class="form-group">
                      <label>Quantité</label>
                      <input class="form-control"  name="quantite" placeholder="saisir quantité"/>
                    </div>
            </div>
            <div class="col-lg-2">
                    <div class="form-group">
                      <label>Dépot secondaire</label>
                      <select name="depot" class="form-control">
                       <% for (var i=0; i < depots.length ; i++) {%>
                           <option value="<%= depots[i].nom %>"><%= depots[i].nom %> </option>
                       <% } %>
                      </select>
                    </div>
            </div>

            <div class="col-lg-2">
                    <div class="form-group">
                      <label>Date</label>
                      <input  class type='form-control' name="date" value="<%= date %>" />
                    </div>
            </div>
         </div>
            <div class="row">
                    <div class="col-lg-3">&nbsp;
                    </div>
                    <div class="col-lg-6">
                        <button type="submit" class="btn btn-default">Enregister</button>
                    </div>

            </form>



        </div> <!-- fin row -->








</div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="../vendor/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="../vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="../vendor/raphael/raphael.min.js"></script>
    <script src="../vendor/morrisjs/morris.min.js"></script>
    <script src="../data/morris-data.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="../dist/js/sb-admin-2.js"></script>



</body>

</html>
