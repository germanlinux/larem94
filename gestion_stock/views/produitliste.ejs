<% include partial_hdp %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Liste des produits</h1>
                </div>






                      <!-- /.col-lg-12 -->
            </div>
            <div class="row">

                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-body">

                            <div class="table-responsive">
                                <table class="table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>
                                                Libellé des produits
                                            </th>
                                            <th>
                                                famille
                                            </th>
                                            <th>
                                                stock dépot
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                     <% produit.forEach(function(i1) { %>
                                        <tr>
                                            <td><%= i1.libelle%></td>
                                            <td><%= i1.famille %></td>
                                            <td><%= i1.stock %></td>
                                        </tr>
                                      <% }); %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
<!--  form creation -->
<div class="row">
            <div class="col-lg-12">
                    <h4 class="page-header">Ajouter un nouveau produit </h4>
           </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
             <div class="panel panel-success">
                <div class="panel-heading">
                            saisir les informations du produits
                </div>
                <form role="form" METHOD="POST">
             </div>
          </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
              <div class="col-lg-8">
                    <div class="form-group">
                      <label>nom du produit</label>
                      <input class="form-control"  name="nom" placeholder="saisir un nom"/>
                    </div>
            </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                      <label>Famille de produit</label>
                      <select name="famille" class="form-control">
                       <% for (var i=0; i < familles.length ; i++) {%>
                           <option value="<%= familles[i].id_famille%>"><%= familles[i].libelle %></option>
                       <% } %>
                      </select>
                </div>
            </div>
            </div>
            <div class="row">
                    <div class="col-lg-3">&nbsp;
                    </div>
                    <div class="col-lg-6">
                        <button type="submit" class="btn btn-default">Enregister</button>
                    </div>
                    <div> &nbsp</div>
                    <div> &nbsp</div>


            </form>



        </div> <!-- fin row -->






<!-- fin creation


        </div>
<!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="../vendor/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="../vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="../vendor/raphael/raphael.min.js"></script>
    <script src="../vendor/morrisjs/morris.min.js"></script>
    <script src="../data/morris-data.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="../dist/js/sb-admin-2.js"></script>

</body>

</html>
